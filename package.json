  "description": "API used to manage standard queries to a DB",
  "main": "dist/app.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "start": "ts-node-dev --respawn -r tsconfig-paths/register  --transpile-only src/app.ts",
