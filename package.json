    "@types/mocha": "^9.1.1",
    "@types/morgan": "^1.9.3",
    "@types/sqlite3": "^3.1.8",
    "chai": "^4.3.6",
    "mocha": "^10.0.0",
    "supertest": "^6.2.3",
    "ts-node": "^10.8.0",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.0.0",
    "tslint": "^6.1.3",
    "typedoc": "^0.24.7",
    "typedoc-plugin-markdown": "^3.15.3",
    "typescript": "^4.7.2"
  },
  "dependencies": {
